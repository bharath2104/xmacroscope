# GraphQL endpoint port
PORT=4000

# Database connection details
DB_HOST=localhost
DB_USERNAME=username
DB_PASSWORD=password
DB_DATABASE=database
# Pick which database type you'll be using
# sqlite, mysql, or postgres
# PostgreSQL is our default
DB_DIALECT=postgres
# Define a filepath for the sqlite storage if that's your storage method
DB_STORAGE=data.sqlite

# Define client origin to allow CORS
CLIENT_ORIGIN=http://localhost:3000
